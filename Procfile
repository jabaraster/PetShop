web:    java -cp src/main/webapp/WEB-INF/classes:target/dependency/* -XX:+UseCompressedOops -javaagent:target/dependency/newrelic-agent.jar -Dmemcached.server=${MEMCACHIER_SERVERS} -Dmemcached.username=${MEMCACHIER_USERNAME} -Dmemcached.password=${MEMCACHIER_PASSWORD} -Dwicket.configuration=deployment -Dweb.port=${PORT} -Ddatabase.url=${DATABASE_URL} -server com.jabaraster.petshop.WebStarter 
